<?xml version="1.0" encoding="UTF-8"?>
<web-app 
 xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
 version="3.0">


 <servlet>
 <servlet-name>Index</servlet-name>
 <servlet-class>com.java.servlets.Index</servlet-class>
 </servlet>
 <servlet>
 <servlet-name>Application</servlet-name>
 <servlet-class>com.java.servlets.Application</servlet-class>
 </servlet>
<servlet>
<servlet-name>FileUploadHandler</servlet-name>
<servlet-class>com.java.servlets.FileUploadHandler</servlet-class>
</servlet>


 <servlet-mapping>
 <servlet-name>Index</servlet-name>
 <url-pattern>/main</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
 <servlet-name>Application</servlet-name>
 <url-pattern>/Connexion</url-pattern>
 </servlet-mapping>
<servlet-mapping>
    <servlet-name>FileUploadHandler</servlet-name>
    <url-pattern>/UploadServlet</url-pattern>
  </servlet-mapping>   

<servlet-mapping>
    <servlet-name>Application</servlet-name>
    <url-pattern>/Compute</url-pattern>
  </servlet-mapping>   

<servlet-mapping>
    <servlet-name>Application</servlet-name>
    <url-pattern>/Play</url-pattern>
  </servlet-mapping>   

</web-app>